{
  "name": "easybird.be",
  "version": "1.0.0",
  "engines": {
    "node": "4.0.0"
  },
  "description": "This is the website of easybird.be",
  "scripts": {
    "start": "forever -w ./bin/www",
    "start-debug": "node ./bin/www",
    "start-prod": "NODE_ENV=production node ./bin/www",
    "check-coverage": "istanbul check-coverage --statements 70 --branches 45 --functions 70 --lines 70",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "NODE_ENV=test mocha -w",
    "test-single": "NODE_ENV=test istanbul cover -x *.test.js _mocha"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "easyblog": "0.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "js-beautify": "1.6.2",
    "lodash": "4.2.0",
    "mongodb": "2.1.7",
    "mongoose": "4.4.3",
    "morgan": "~1.6.1",
    "node-mongoose-fixtures": "0.2.4",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "istanbul": "0.4.3",
    "mocha": "2.4.5",
    "supertest": "1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/easybird/easybird.be.git"
  },
  "keywords": [
