{
  "name": "easybird.be",
  "version": "1.0.0",
  "engines": {
    "node": "4.0.0"
  },
  "description": "This is the website of easybird.be",
  "scripts": {
    "start": "node ./bin/www",
    "auto-start": "forever -w ./bin/www",
    "start-prod": "NODE_ENV=production node ./bin/www",
    "check-coverage": "istanbul check-coverage --statements 10 --branches 10 --functions 10 --lines 10",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "NODE_ENV=test mocha -w --compilers js:babel-register --recursive",
    "test-single": "NODE_ENV=test istanbul cover -x *.test.js _mocha -- --compilers js:babel-register --recursive"
  },
  "dependencies": {
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-register": "6.8.0",
    "bluebird": "3.3.5",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "easyblog": "0.4.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "js-beautify": "1.6.2",
    "lodash": "4.2.0",
    "material-ui": "0.15.0-beta.2",
    "mongodb": "2.1.7",
    "mongoose": "4.4.3",
    "morgan": "~1.6.1",
    "node-mongoose-fixtures": "0.2.4",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "react-tap-event-plugin": "1.0.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "istanbul": "0.4.3",
    "mocha": "2.4.5",
    "supertest": "1.2.0",
    "webpack": "1.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/easybird/easybird.be.git"
  },
  "keywords": [
    "easybird"
  ],
  "license": "MIT",
  "author": "easybird.be <jonathan@easybird.be> (http://easybird.be/)",
  "bugs": {
    "url": "https://github.com/easybird/easybird.be/issues"
  },
  "homepage": "https://github.com/easybird/easybird.be#readme"
}
